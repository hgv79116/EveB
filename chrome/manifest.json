{ 
    "manifest_version": 2, 
    "name": "EveB", 
    "version": 1.0, 
    "description": "Automatically parse highlighted text into events", 
    "action": { 
        "default_popup": "popup/popup.html"
    }, 
    "background": { 
        "service_worker": "background.js"
    }, 
    "permissions": [ 
        "contextMenus"
    ]
}