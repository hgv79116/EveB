{ 
    "manifest_version": 3, 
    "name": "EveB", 
    "version": "1.0", 
    "description": "Automatically parse highlighted text into events", 
    "action": { 
        "default_popup": "popup/popup.html"
    }, 
    "background": { 
        "service_worker": "background.js"
    }, 
    "permissions": [ 
        "contextMenus", 
        "notifications"
    ], 
    "host_permissions": [
        "http://127.0.0.1:5000/*"
    ]
}